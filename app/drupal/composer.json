{
    "name": "yzhao/drupal-test",
    "description": "test drupal 8 with composer",
    "authors": [
        {
            "name": "YannKNX",
            "email": "yzhao@kernix.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "drupal-composer/drupal-scaffold": "^2.0@dev",
        "composer/installers": "^1.0@dev",
        "drupal/admin_toolbar": "^1.27",
        "drupal/redirect": "^1.3",
        "drupal/adminimal_theme": "^1.5",
        "drupal/bootstrap_barrio": "^4.22",
        "drupal/business_responsive_theme": "^1.3",
        "drupal/devel": "^2.1"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
